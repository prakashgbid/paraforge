# Jira Configuration
JIRA_HOST=your-domain.atlassian.net
JIRA_EMAIL=your@email.com
JIRA_API_TOKEN=your_jira_api_token_here

# AI Configuration
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GEMINI_API_KEY=AIza...

# ParaForge Configuration
PARAFORGE_MODE=development
PARAFORGE_LOG_LEVEL=debug
PARAFORGE_PORT=3000
PARAFORGE_CACHE_ENABLED=true
PARAFORGE_CACHE_TTL=3600

# Database (Optional - for learning system)
DATABASE_URL=postgresql://user:password@localhost:5432/paraforge

# Redis (Optional - for caching)
REDIS_URL=redis://localhost:6379

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Feature Flags
ENABLE_LEARNING_SYSTEM=true
ENABLE_PATTERN_RECOGNITION=true
ENABLE_AUTO_OPTIMIZATION=true